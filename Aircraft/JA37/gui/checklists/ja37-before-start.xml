        <item>
            <name>Master caution</name>
            <value>RESET</value>
            <marker>
                <x-m>-4.0446</x-m>
                <y-m>-0.1719</y-m>
                <z-m>0.5165</z-m>
                <scale>1.0000</scale>
            </marker>
            <condition>
                <less-than-equals>
                    <property>fdm/jsbsim/systems/indicators/master-warning/warning</property>
                    <value>1</value>
                </less-than-equals>
            </condition>
            <binding>
                <command>nasal</command>
                <script>
                  setprop("ja37/avionics/master-warning-button", 1);
                  ja37.click();
                  var timer = maketimer(0.3, func(){
                      setprop("ja37/avionics/master-warning-button", 0);
                  });
                  timer.singleShot = 1;
                  timer.simulatedTime = 1;
                  timer.start();
                </script>
            </binding>
        </item>
