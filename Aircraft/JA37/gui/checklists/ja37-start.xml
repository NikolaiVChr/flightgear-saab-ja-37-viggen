<?xml version="1.0"?>

<PropertyList>
    <title>Start Engine</title>
    <item>
        <name>Hold engine starter (2 sec)</name>
        <value>ON</value>
        <marker>
            <x-m>-3.7322</x-m>
            <y-m>-0.4727</y-m>
            <z-m>0.1116</z-m>
            <scale>3.0000</scale>
        </marker>
        <condition>
            <property>fdm/jsbsim/systems/electrical/starter-relay-R1</property>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("controls/engines/engine[0]/starter-cmd-hold", 1);
              setprop("controls/engines/engine[0]/starter-cmd", 1);
              ja37.click();
              var timer = maketimer(2.1, func(){
                  setprop("controls/engines/engine[0]/starter-cmd-hold", 0);
              });
              timer.singleShot = 1;
              timer.simulatedTime = 1;
              timer.start();
            </script>
        </binding>
    </item>
    <item><name>Check:</name></item>
    <item>
        <name>(a) Start sys indicator</name>
        <value>LIT WITHIN 5s</value>
        <condition>
            <property>fdm/jsbsim/systems/indicators/start-system</property>
        </condition>
        <marker>
            <x-m>-3.8650</x-m>
            <y-m>0.2650</y-m>
            <z-m>0.1200</z-m>
            <scale>1.0000</scale>
        </marker>
    </item>
    <item>
        <name>(b) Exhaust gas temperature</name>
        <value>MAX 485C (>415C MAX 15s)</value>
        <condition>
            <less-than>
              <property>fdm/jsbsim/propulsion/engine/outlet-temperature-degc</property>
              <value>485</value>
            </less-than>
        </condition>
        <marker>
            <x-m>-3.8000</x-m>
            <y-m>0.2800</y-m>
            <z-m>0.1100</z-m>
            <scale>1.7000</scale>
        </marker>
    </item>
    <item>
        <name>If (a) or (b) exceeded, or quick acceleration past ground idle, or if RPM remains &lt;54%</name>
    </item>
    <item>
        <name>Throttle (click to lift, then pull back)</name>
        <value>CUTOFF</value>
        <marker>
            <x-m>-3.6000</x-m>
            <y-m>-0.4587</y-m>
            <z-m>0.0959</z-m>
            <scale>7.0000</scale>
        </marker>
        <condition>
            <less-than>
                <property>/fdm/jsbsim/fcs/throttle-pos-deg</property>
                <value>7</value>
            </less-than>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>/fdm/jsbsim/fcs/throttle-cutoff-catch</property>
            <value>0</value>
        </binding>
        <binding>
            <command>property-assign</command>
            <property>/controls/engines/engine[0]/throttle</property>
            <value>0</value>
        </binding>
    </item>
    <item><name>At ground idle, check:</name></item>
    <item>
        <name>Fan RPM</name>
        <value>54-60%</value>
        <condition>
            <and>
                <greater-than>
                    <property>/engines/engine[0]/n2</property>
                    <value>54</value>
                </greater-than>
                <less-than>
                    <property>/engines/engine[0]/n2</property>
                    <value>60</value>
                </less-than>
            </and>
        </condition>
        <marker>
            <x-m>-3.9678</x-m>
            <y-m>-0.1900</y-m>
            <z-m>0.2471</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Engine pressure ratio</name>
        <value>AROUND 1.0</value>
        <condition>
            <less-than>
              <property>/engines/engine[0]/epr-gauge</property>
              <value>1.1</value>
            </less-than>
        </condition>
        <marker>
            <x-m>-3.9678</x-m>
            <y-m>-0.1331</y-m>
            <z-m>0.2471</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Nozzle Position</name>
        <value>FULL OPEN</value>
        <condition>
            <greater-than>
              <property>/fdm/jsbsim/propulsion/engine/nozzle</property>
              <value>0.95</value>
            </greater-than>
        </condition>
        <marker>
            <x-m>-3.8000</x-m>
            <y-m>0.3300</y-m>
            <z-m>0.1100</z-m>
            <scale>1.7000</scale>
        </marker>
    </item>
    <item>
        <name>Exhaust gas temperature</name>
        <value>MAX 385 C</value>
        <condition>
            <less-than>
              <property>fdm/jsbsim/propulsion/engine/outlet-temperature-degc</property>
              <value>385</value>
            </less-than>
        </condition>
        <marker>
            <x-m>-3.8000</x-m>
            <y-m>0.2800</y-m>
            <z-m>0.1100</z-m>
            <scale>1.7000</scale>
        </marker>
    </item>
    <item>
        <name>Warning lights</name>
        <value>CHECK OFF (X-Tank Fuel off at 70%rpm)</value>
    </item>
    <item>
        <name>Instruments Failure Flags</name>
        <value>CHECK OFF</value>
    </item>
    <item>
        <name>SPAK</name>
        <value>CHECK ON</value>
        <marker>
            <x-m>-4.0400</x-m>
            <y-m>-0.2420</y-m>
            <z-m>0.4550</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <property>/instrumentation/joystick-indicator</property>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("ja37/avionics/button-stck", 1);
              auto.mode1();
              ja37.click();
              var timer = maketimer(0.3, func(){
                  setprop("ja37/avionics/button-stck", 0);
              });
              timer.singleShot = 1;
              timer.simulatedTime = 1;
              timer.start();
            </script>
        </binding>
    </item>
    <item>
        <name>Toggle external power (menu)</name>
        <value>OFF</value>
        <binding>
            <command>property-assign</command>
            <property>fdm/jsbsim/systems/electrical/external/enable-cmd</property>
            <value>0</value>
        </binding>
        <condition>
            <not><property>fdm/jsbsim/systems/electrical/external/enable-cmd</property></not>
        </condition>
    </item>
</PropertyList>
