<?xml version="1.0"?>

<PropertyList>
    <title>Generator/Electrical Failure</title>
    <item>
        <name>Attempt generator restart ONCE:</name>
        <value></value>
    </item>
    <item>
        <name>Generator</name>
        <value>CYCLE OFF/ON</value>
        <marker>
            <x-m>-3.7223</x-m>
            <y-m>-0.4240</y-m>
            <z-m>0.1050</z-m>
            <scale>3.0000</scale>
        </marker>
        <condition>
            <property>fdm/jsbsim/systems/electrical/generator-ready</property>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("/controls/electric/engine[0]/generator", 0);
              ja37.click();
              var timer = maketimer(1, func(){
                  setprop("/controls/electric/engine[0]/generator", 1);
                  ja37.click();
              });
              timer.singleShot = 1;
              timer.simulatedTime = 1;
              timer.start();
            </script>
        </binding>
    </item>
    <item>
        <name>Cancel Mission</name>
        <value></value>
    </item>
    <item>
        <name>If failure persists, emergency ram air turbine</name>
        <value>ON</value>
        <marker>
            <x-m>-3.5868</x-m>
            <y-m>0.4430</y-m>
            <z-m>0.2223</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <property>controls/electric/reserve</property>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>controls/electric/reserve</property>
            <value>1</value>
        </binding>
        <binding>
            <command>nasal</command>
            <script>ja37.click();</script>
        </binding>
    </item>
</PropertyList>
