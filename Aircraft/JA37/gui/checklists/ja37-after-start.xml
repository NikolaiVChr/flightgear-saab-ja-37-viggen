<?xml version="1.0"?>

<PropertyList>
    <title>After Battery Engine Start</title>
    <item>
        <name>If engine started on battery only (no external power), check:</name>
    </item>
    <item>
        <name>TI display</name>
        <value>ON, BRIGHTNESS AND CONTRAST SET</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>0.1800</y-m>
            <z-m>0.3400</z-m>
            <scale>8.0</scale>
        </marker>
    </item>
    <item>
        <name>Backup altimeter</name>
        <value>CHECK</value>
        <marker>
            <x-m>-3.9900</x-m>
            <y-m>-0.2700</y-m>
            <z-m>0.2300</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Attitude indicator</name>
        <value>CHECK</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>-0.1520</y-m>
            <z-m>0.3400</z-m>
            <scale>4.0000</scale>
        </marker>
    </item>
    <item>
        <name>Backup heading</name>
        <value>ENABLE, CHECK HEADING</value>
        <marker>
            <x-m>-4.0554</x-m>
            <y-m>0.1669</y-m>
            <z-m>0.5182</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <property>ja37/avionics/reservkurs</property>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>ja37/avionics/reservkurs</property>
            <value>1</value>
        </binding>
    </item>
    <item>
        <name>Backup heading</name>
        <value>DISABLE</value>
        <marker>
            <x-m>-4.0554</x-m>
            <y-m>0.1669</y-m>
            <z-m>0.5182</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <not><property>ja37/avionics/reservkurs</property></not>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>ja37/avionics/reservkurs</property>
            <value>0</value>
        </binding>
    </item>
    <item>
        <name>Fuel indicator</name>
        <value>CHECK</value>
        <marker>
            <x-m>-3.9504</x-m>
            <y-m>0.3256</y-m>
            <z-m>0.2405</z-m>
            <scale>2.1074</scale>
        </marker>
    </item>
    <item>
        <name>Data input/download</name>
        <value>DONE</value>
        <marker>
            <x-m>-3.7000</x-m>
            <y-m>0.3000</y-m>
            <z-m>0.1000</z-m>
            <scale>6.0</scale>
        </marker>
    </item>
    <item>
        <name>Mission data/RTB routes</name>
        <value>CHECK</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>0.1800</y-m>
            <z-m>0.3400</z-m>
            <scale>8.0</scale>
        </marker>
    </item>
    <item>
        <name>Mission/RTB polygons</name>
        <value>SET</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>0.1800</y-m>
            <z-m>0.3400</z-m>
            <scale>8.0</scale>
        </marker>
    </item>
    <item>
        <name>TI Display options</name>
        <value>SET</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>0.1800</y-m>
            <z-m>0.3400</z-m>
            <scale>8.0</scale>
        </marker>
    </item>
    <item>
        <name>Oxygen</name>
        <value>ON, PRESSURE MIN 90</value>
        <marker>
            <x-m>-3.8794</x-m>
            <y-m>0.4016</y-m>
            <z-m>0.1570</z-m>
            <scale>1.0000</scale>
        </marker>
        <binding>
            <command>property-assign</command>
            <property>controls/oxygen</property>
            <value>1</value>
        </binding>
        <condition>
            <property>controls/oxygen</property>
        </condition>
    </item>
</PropertyList>
