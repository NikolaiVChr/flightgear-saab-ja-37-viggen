<?xml version="1.0"?>

<PropertyList>
    <title>After Battery Engine Start</title>
    <item>
        <name>If engine started on battery only (no external power), check:</name>
    </item>
    <item>
        <name>Backup altimeter</name>
        <value>CHECK</value>
        <marker>
            <x-m>-3.9901</x-m>
            <y-m>0.1200</y-m>
            <z-m>0.3250</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Attitude indicator</name>
        <value>CHECK</value>
        <marker>
            <x-m>-4.0300</x-m>
            <y-m>-0.1400</y-m>
            <z-m>0.4000</z-m>
            <scale>4.0000</scale>
        </marker>
    </item>
    <item>
        <name>Backup heading</name>
        <value>CHECK</value>
        <marker>
            <x-m>-3.9800</x-m>
            <y-m>0.1200</y-m>
            <z-m>0.2500</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Fuel indicator</name>
        <value>CHECK</value>
        <marker>
            <x-m>-3.9504</x-m>
            <y-m>0.2400</y-m>
            <z-m>0.2900</z-m>
            <scale>4</scale>
        </marker>
    </item>
    <item>
        <name>Oxygen</name>
        <value>ON, PRESSURE MIN 90</value>
        <marker>
            <x-m>-3.8794</x-m>
            <y-m>0.4016</y-m>
            <z-m>0.1570</z-m>
            <scale>1.0000</scale>
        </marker>
        <binding>
            <command>property-assign</command>
            <property>controls/oxygen</property>
            <value>1</value>
        </binding>
        <condition>
            <property>controls/oxygen</property>
        </condition>
    </item>
</PropertyList>
