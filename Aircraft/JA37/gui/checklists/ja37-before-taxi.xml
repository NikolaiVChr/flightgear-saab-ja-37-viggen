<?xml version="1.0"?>

<PropertyList>
    <title>Before Taxi</title>
    <item>
        <name>Control surfaces</name>
        <value>CHECK MOVEMENT</value>
    </item>
    <item>
        <name>Pitch trim</name>
        <value>CHECK BETWEEN -3/0</value>
        <marker>
            <x-m>-3.8350</x-m>
            <y-m>-0.4116</y-m>
            <z-m>0.1612</z-m>
            <scale>2.0000</scale>
        </marker>
        <condition>
            <and>
                <greater-than-equals>
                    <property>/controls/flight/elevator-trim</property>
                    <value>-0.3</value>
                </greater-than-equals>
                <less-than-equals>
                    <property>/controls/flight/elevator-trim</property>
                    <value>0</value>
                </less-than-equals>
            </and>
        </condition>
        <binding>
            <command>property-assign</command>n
            <property>/controls/flight/elevator-trim</property>
            <value>0</value>
        </binding>
    </item>
    <item>
        <name>Brake Pressure</name>
        <value>200-270 kp/cm2</value>
        <marker>
            <x-m>-3.900</x-m>
            <y-m>-0.4116</y-m>
            <z-m>0.1752</z-m>
            <scale>2.0000</scale>
        </marker>
        <condition>
            <and>
                <greater-than-equals>
                    <property>fdm/jsbsim/instruments/brake-pressure/indicated-pressure-kp_cm2</property>
                    <value>200</value>
                </greater-than-equals>
                <less-than-equals>
                    <property>fdm/jsbsim/instruments/brake-pressure/indicated-pressure-kp_cm2</property>
                    <value>270</value>
                </less-than-equals>
            </and>
        </condition>
    </item>
    <item>
        <name>Calibrate altimeters</name>
        <value>SET</value>
        <marker>
            <x-m>-4.0000</x-m>
            <y-m>-0.2500</y-m>
            <z-m>0.3400</z-m>
            <scale>3.0000</scale>
        </marker>
        <condition>
            <and>
                <not><property>instrumentation/altimeter/setting-std</property></not>
                <less-than>
                    <property>instrumentation/altimeter/indicated-altitude-meter</property>
                    <value>10</value>
                </less-than>
                <greater-than>
                    <property>instrumentation/altimeter/indicated-altitude-meter</property>
                    <value>-10</value>
                </greater-than>
            </and>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("/instrumentation/altimeter/setting-std", 0);
              setprop("/instrumentation/altimeter/setting-inhg",
                      getprop("/environment/pressure-inhg"));
            </script>
        </binding>
    </item>
    <item>
        <name>Master caution</name>
        <value>CHECK OFF</value>
        <marker>
            <x-m>-4.0446</x-m>
            <y-m>-0.1719</y-m>
            <z-m>0.5165</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <equals>
                <property>fdm/jsbsim/systems/indicators/master-warning/visual</property>
                <value>0</value>
            </equals>
        </condition>
    </item>
    <item>
        <name>Warning lights</name>
        <value>CHECK OFF (X-Tank Fuel off at 70%rpm)</value>
    </item>
    <item>
        <name>SPAK</name>
        <value>CHECK ON</value>
        <marker>
            <x-m>-4.0400</x-m>
            <y-m>-0.2420</y-m>
            <z-m>0.4550</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <property>fdm/jsbsim/autoflight/stick-light-cmd</property>
        </condition>
        <binding>
            <command>nasal</command>
            <script>
              setprop("ja37/avionics/button-stck", 1);
              autoflight.System.engageMode(1);
              ja37.click();
              settimer(func { setprop("ja37/avionics/button-stck", 0); }, 0.3);
            </script>
        </binding>
    </item>
    <item>
        <name>Inertia nav initialized (takes a few minutes)</name>
        <value>CHECK, INDICATOR SOLID</value>
        <marker>
            <x-m>-3.9900</x-m>
            <y-m>0.0800</y-m>
            <z-m>0.3000</z-m>
            <scale>1.0000</scale>
        </marker>
        <condition>
            <property>ja37/avionics/ins</property>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>ja37/avionics/ins-cmd</property>
            <value>1</value>
        </binding>
    </item>
    <item>
        <name>Transponder</name>
        <value>CODE SET</value>
        <marker>
            <x-m>-3.4174</x-m>
            <y-m>0.32</y-m>
            <z-m>-0.0099</z-m>
            <scale>3.0000</scale>
        </marker>
    </item>
    <item>
        <name>Transponder</name>
        <value>OFF</value>
        <condition>
            <equals>
                <property>/instrumentation/transponder/switch-power</property>
                <value>0</value>
            </equals>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>/instrumentation/transponder/switch-power</property>
            <value>0</value>
        </binding>
        <marker>
            <x-m>-3.4174</x-m>
            <y-m>0.2893</y-m>
            <z-m>-0.0099</z-m>
            <scale>1.0000</scale>
        </marker>
    </item>
    <item>
        <name>Radio talk setting</name>
        <value>A</value>
        <marker>
            <x-m>-3.4438</x-m>
            <y-m>-0.3306</y-m>
            <z-m>-0.0000</z-m>
            <scale>1.5124</scale>
        </marker>
        <condition>
            <equals>
                <property>instrumentation/comm[0]/transmitter</property>
                <value>0</value>
            </equals>
        </condition>
        <binding>
            <command>property-assign</command>
            <property>instrumentation/comm[0]/transmitter</property>
            <value>0</value>
        </binding>
    </item>
</PropertyList>
