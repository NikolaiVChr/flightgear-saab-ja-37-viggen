		<!-- custom keyboard bindings -->
		
		<key n="125">
          <name>}</name>
          <desc>Gross Freq Increase</desc>
		  <repeatable>true</repeatable>
			<interval-sec>0.1</interval-sec>
          	<binding>
				<condition>
					<and>
						<equals>
							<property>instrumentation/radio/switches/com-nav</property>
							<value>0</value>
						</equals>
						<equals>
							<property>instrumentation/radio/switches/mhz-khz</property>
							<value>0</value>
						</equals>
					</and>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/comm/frequencies/selected-mhz</property>
				<step>1</step>
				<min>118</min>
				<max>137</max>
				<wrap>true</wrap>
			</binding>
			<!-- binding for if knob is on vor/ils/dme nav -->
			<binding>
				<condition>
					<and>
						<equals>
							<property>instrumentation/radio/switches/com-nav</property>
							<value>1</value>
						</equals>
						<equals>
							<property>instrumentation/radio/switches/mhz-khz</property>
							<value>0</value>
						</equals>
					</and>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/nav/frequencies/selected-mhz</property>
				<step>1</step>
				<min>100</min>
				<max>118</max>
				<wrap>true</wrap>
			</binding>
			<!-- binding for if knob is on adf -->
			<binding>
				<condition>
					<and>
						<equals>
							<property>instrumentation/radio/switches/com-nav</property>
							<value>1</value>
						</equals>
						<equals>
							<property>instrumentation/radio/switches/mhz-khz</property>
							<value>1</value>
						</equals>
					</and>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/adf/frequencies/selected-khz</property>
				<step>1</step>
				<min>100</min>
				<max>500</max>
				<wrap>true</wrap>
			</binding>
			<binding>
                <command>nasal</command>
                <script>ja37.click()</script>
            </binding>
         </key>
		 
