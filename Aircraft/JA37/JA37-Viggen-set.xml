                <menu n="100">
                    <label>JA-37Di</label>
                    <enabled type="bool">true</enabled>
                    <item>
                        <label>Select Livery</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                aircraft.livery.dialog.toggle();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Auto start/stop</label>
                        <binding>
                            <command>nasal</command>
                            <script>ja37.autostarttimer();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Repair</label>
                        <binding>
                            <command>nasal</command>
                            <script>ja37.repair();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Reload air to air (1979)</label>
                        <binding>
                            <command>nasal</command>
                            <script>armament.reloadJAAir2Air1979();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Reload air to air (1987)</label>
                        <binding>
                            <command>nasal</command>
                            <script>armament.reloadJAAir2Air1987();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Reload air to air (1997)</label>
                        <binding>
                            <command>nasal</command>
                            <script>armament.reloadJAAir2Air1997();</script>
                        </binding>
                    </item>
					<item>
                        <label>Reload guns only</label>
                        <binding>
                            <command>nasal</command>
                            <script>armament.reloadGuns();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Reload air to ground</label>
                        <binding>
                            <command>nasal</command>
                            <script>armament.reloadJAAir2Ground();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Refuel: Standard</label>
                        <binding>
                            <command>nasal</command>
                            <script>ja37.refuelNorm();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Refuel: Long distance</label>
                        <binding>
                            <command>nasal</command>
                            <script>ja37.refuelRange();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Refuel: Test flight</label>
                        <binding>
                            <command>nasal</command>
                            <script>ja37.refuelTest();</script>
                        </binding>
                    </item>
                    <item>
                        <label>Performance Monitor</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                  monitor.PerformanceMonitor.instance().start();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Systems Monitor</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                monitor2.SystemsMonitor.instance().start();
                            </script>
                        </binding>
                    </item>
                    <item>
                      <label>Friends and Foes</label>
                      <name>faf</name>
                      <binding>
                        <command>dialog-show</command>
                        <dialog-name>faf</dialog-name>
                      </binding>
                    </item>
                    <item>
                        <label>Toggle external power</label>
                        <binding>
                            <command>property-toggle</command>
                            <property>fdm/jsbsim/systems/electrical/external/enable-cmd</property>
                        </binding>
                    </item>
                    <item>
                        <label>Options</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                ja37.Dialog.show();
                            </script>
                        </binding>
                    </item>
