	<!--
	**********************************
	Mode Knob
	**********************************
	-->
	<!-- rotate by 32* -->
	<animation>
        <type>knob</type>
        <object-name>modeSelectKnob</object-name>
		<property>ja37/navigation/dp-mode</property>
		<factor>30</factor>
		<center>
			<x-m>  0.01251 </x-m>
			<y-m> -0.02351 </y-m>
			<z-m>  0.00000 </z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>ja37/navigation/dp-mode</property>
                <min>0</min>
                <max>6</max>           
                <wrap>false</wrap>
           </binding>
        </action>
    </animation>
	
	<!--
	**********************************
	Clear Button Cover
	**********************************
	-->
	
	<animation>
		<type>pick</type>
		<object-name>rensaCover</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>ja37/navigation/rensa-cover</property>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>rensaCover</object-name>
		<property>ja37/navigation/rensa-cover</property>
		<factor>135</factor>
		<center>
			<x-m> 0   </x-m>
			<y-m> -0.00248   </y-m>
			<z-m> 0.00824 </z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
