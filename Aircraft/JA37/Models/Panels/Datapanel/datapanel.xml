			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>dap.test();</script>
			</binding>
		</action>
		<hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>rs</tooltip-id>
                <label>Button: TEST</label>
            </binding>
        </hovered>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>posNegBT</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>dap.togglePN();</script>
			</binding>
		</action>
		<hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>pn</tooltip-id>
                <label>Button: POSITIVE or NEGATIVE</label>
            </binding>
        </hovered>
	</animation>

	<animation>
        <type>material</type>
        <object-name>dPos</object-name>
        <emission>
            <red-prop>controls/lighting/displays-norm</red-prop>
            <green-prop>controls/lighting/displays-norm</green-prop>
            <blue-prop>controls/lighting/displays-norm</blue-prop>
            <factor-prop>ja37/dap/pos</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>dM</object-name>
        <emission>
            <red-prop>controls/lighting/displays-norm</red-prop>
            <green-prop>controls/lighting/displays-norm</green-prop>
            <blue-prop>controls/lighting/displays-norm</blue-prop>
            <factor-prop>ja37/dap/msda</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>dIn</object-name>
        <emission>
            <red-prop>controls/lighting/displays-norm</red-prop>
            <green-prop>controls/lighting/displays-norm</green-prop>
            <blue-prop>controls/lighting/displays-norm</blue-prop>
            <factor-prop>ja37/dap/in</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>dOut</object-name>
        <emission>
            <red-prop>controls/lighting/displays-norm</red-prop>
            <green-prop>controls/lighting/displays-norm</green-prop>
            <blue-prop>controls/lighting/displays-norm</blue-prop>
            <factor-prop>ja37/dap/out</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>select</type>
        <condition>
	       <equals>
	        <property>ja37/systems/variant</property>
	        <value>0</value>
	       </equals>
	     </condition>
        <object-name>DataCanvas</object-name>
        <object-name>mountMinor</object-name>
        <object-name>mountMajor</object-name>
        <object-name>mountDisplay</object-name>
        <object-name>dIn</object-name>
        <object-name>dOut</object-name>
        <object-name>dM</object-name>
        <object-name>dPos</object-name>
        <object-name>reset</object-name>
        <object-name>posNegBT</object-name>
    </animation>

    <animation>
        <type>select</type>
        <condition>
	       <greater-than>
	        <property>ja37/systems/variant</property>
	        <value>0</value>
	       </greater-than>
	     </condition>
        <object-name>1Data</object-name>
        <object-name>2Data</object-name>
        <object-name>3Data</object-name>
        <object-name>4Data</object-name>
        <object-name>5Data</object-name>
        <object-name>6Data</object-name>
        <object-name>7Data</object-name>
        <object-name>inOutSwitch</object-name>
        <object-name>posNegSwitch</object-name>
    </animation>
	
</PropertyList>
