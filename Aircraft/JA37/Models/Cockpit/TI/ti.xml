    <!--


							INDICATORS


  	-->

    <animation>
        <type>material</type>
        <object-name>light_green_right</object-name>
        <object-name>light_green_left</object-name>
        <condition>
            <property>/controls/lighting/test-indicator-panels</property>
        </condition>
        <emission>
            <red>0</red> 
            <green>1</green> 
            <blue>0</blue>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
        </emission>
    </animation>


