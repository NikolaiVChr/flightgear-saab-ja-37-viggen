  	<!--


							flipper buttons


  	-->

  	<animation>
        <type>pick</type>
        <object-name>button_brightness_plus</object-name>
        <visible>true</visible>
        <action>
            <name>brightness</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>TI.brightnessP();</script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>flipperBp</tooltip-id>
                <label>TI: Brightness</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>button_brightness_minus</object-name>
        <visible>true</visible>
        <action>
            <name>brightness</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>TI.brightnessM();</script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>flipperB</tooltip-id>
                <label>TI: Brightness</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>pick</type>
