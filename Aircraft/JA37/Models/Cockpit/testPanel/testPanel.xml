            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>test-start</tooltip-id>
                <label>Start testing. Acknowledge test result. Interrupt and skip to next testing.</label>
            </binding>
        </hovered>
    </animation>
    <animation>
       <type>translate</type>
       <object-name>btStart</object-name>
       <object-name>btStarttp</object-name>
