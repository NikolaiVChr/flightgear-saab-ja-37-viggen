<?xml version="1.0"?>

<PropertyList>

    <path>testPanel.ac</path>

    <effect>
        <inherits-from>Aircraft/JA37/Models/Effects/ja37-cockpit-shader</inherits-from>
        <object-name>knob</object-name>
        <object-name>plane</object-name>
        <object-name>A2_disc</object-name>
        <object-name>A2_handle</object-name>
        <object-name>A2_screw</object-name>
        <object-name>btYM</object-name>
        <object-name>btStart</object-name>
        <object-name>btREP</object-name>
        <object-name>btIPK</object-name>
        <object-name>btFK</object-name>
        <object-name>btFEL</object-name>
        <object-name>btYMtp</object-name>
        <object-name>btStarttp</object-name>
        <object-name>btREPtp</object-name>
        <object-name>btIPKtp</object-name>
        <object-name>btFKtp</object-name>
        <object-name>btFELtp</object-name>
    </effect>

    <animation>
        <type>pick</type>
        <object-name>plane</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>knob-windshield-hot-air</tooltip-id>
                <label>Test panel and Hot air on windshield</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>knob</object-name>
        <property>controls/ventilation/windshield-hot-air-knob</property>
        <factor>270</factor>
        <offset-deg>-135</offset-deg>
        <center>
            <x-m>  0.00 </x-m>
            <y-m>  0.00 </y-m>
            <z-m>  0.00 </z-m>
        </center>
        <axis>
            <x> 0.0 </x>
            <y> 0.0 </y>
            <z> -1.0 </z>
        </axis>
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>controls/ventilation/windshield-hot-air-knob</property>
                <factor>1</factor>
                <step>0.2</step>
                <min>0</min>
                <max>1</max>           
                <wrap>false</wrap>
           </binding>
        </increase>
        <decrease>
            <binding>
                <command>property-adjust</command>
                <property>controls/ventilation/windshield-hot-air-knob</property>
                <factor>1</factor>
                <step>-0.2</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </decrease>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>btYM</object-name>
        <object-name>btYMtp</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>ja37/test/button-ym</property>
                <value>1</value>
            </binding>
            <binding>
                <command>nasal</command>
                <script>testing.press(1);</script>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>ja37/test/button-ym</property>
                    <value>0</value>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>test-ym</tooltip-id>
                <label>YM</label>
            </binding>
        </hovered>
    </animation>
    <animation>
       <type>translate</type>
       <object-name>btYM</object-name>
       <object-name>btYMtp</object-name>
       <property>ja37/test/button-ym</property>
       <factor>0.005</factor>
       <offset-m>0</offset-m>
       <axis>
         <z>-1</z>
       </axis>
       <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>
    <animation>
       <type>material</type>
       <object-name>btYMtp</object-name>
       <emission>
            <red>1.0</red> 
            <green>1.0</green> 
            <blue>1.0</blue>
            <factor-prop>ja37/test/button-ym-tp</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>btStart</object-name>
        <object-name>btStarttp</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>ja37/test/button-start</property>
                <value>1</value>
            </binding>
            <binding>
                <command>nasal</command>
