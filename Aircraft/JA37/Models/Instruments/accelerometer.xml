  <animation>
    <object-name>accel</object-name> 
    <type>material</type>
    <condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>0.55</red>
      <green>0.24</green>
      <blue>0.10</blue>
    </emission>
  </animation>

