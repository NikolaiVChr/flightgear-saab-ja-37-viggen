<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>

    <path>transponder.ac</path>

    <effect>
        <inherits-from>Aircraft/JA37/Models/Effects/ja37-cockpit-shader</inherits-from>
        <object-name>panel</object-name>
        <object-name>ident</object-name>
        <!--<object-name>lampRespond</object-name>
        <object-name>lampErr</object-name>-->
        <object-name>Border</object-name>
        <object-name>test</object-name>
        <object-name>Dial1</object-name>
        <object-name>Dial10</object-name>
        <object-name>Dial100</object-name>
        <object-name>Dial1000</object-name>
        <object-name>Knob1</object-name>
        <object-name>Knob10</object-name>
        <object-name>Knob100</object-name>
        <object-name>Knob1000</object-name>
        <object-name>switchMode</object-name>
        <object-name>switchEnable</object-name>
    </effect>

    <effect>
        <inherits-from>Aircraft/JA37/Models/Effects/ja37-cockpit-transparent-shader</inherits-from>
        <object-name>glass</object-name>
    </effect>

    <animation>
        <type>noshadow</type>
        <object-name>glass</object-name>
    </animation>

    <params>
        <digit0>instrumentation/transponder/inputs/digit[0]</digit0>
        <digit1>instrumentation/transponder/inputs/digit[1]</digit1>
        <digit2>instrumentation/transponder/inputs/digit[2]</digit2>
        <digit3>instrumentation/transponder/inputs/digit[3]</digit3>
    </params>

    <animation>
        <type>pick</type>
        <object-name>panel</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>trs</tooltip-id>
                <label>Transponder</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>material</type>
        <object-name>Dial1</object-name>
        <object-name>Dial10</object-name>
        <object-name>Dial100</object-name>
        <object-name>Dial1000</object-name>
        <condition>
            <equals>
                <property>/instrumentation/transponder/inputs/knob-mode</property>
                <value>5</value>
            </equals>
        </condition>
        <emission>
            <red-prop>instrumentation/instrumentation-light/r</red-prop>
            <green-prop>instrumentation/instrumentation-light/g</green-prop>
            <blue-prop>instrumentation/instrumentation-light/b</blue-prop>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Dial1000</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[3]</property>-->
        <property alias="../../params/digit3"/>
        <factor>45</factor>
        <center>
            <z-m>  0.010171 </z-m>
            <y-m> -0.01295 </y-m>
            <x-m>  0.00079415 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>Knob1000</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[3]</property>-->
        <property alias="../../params/digit3"/>
        <factor>45</factor>
        <center>
            <z-m>  0.010171 </z-m>
            <y-m> -0.01295 </y-m>
            <x-m> -0.0004585 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
        <visible>true</visible>
        <drag-scale-px>20</drag-scale-px>
        <action>
            <binding>
                <command>property-adjust</command>
                <!--<property>instrumentation/transponder/inputs/digit[3]</property>-->
                <property alias="../../../../params/digit3"/>
                <factor>1</factor>
                <min>0</min>
                <max>8</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-squawk</tooltip-id>
                <label>Squawk: %.4d</label>
                <property>instrumentation/transponder/id-code</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Dial100</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[2]</property>-->
        <property alias="../../params/digit2"/>
        <factor>45</factor>
        <center>
            <z-m>  0.0028 </z-m>
            <y-m>  0.00595 </y-m>
            <x-m>  0.00079415 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>Knob100</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[2]</property>-->
        <property alias="../../params/digit2"/>
        <factor>45</factor>
        <center>
            <z-m>  0.010171 </z-m>
            <y-m>  0.00595 </y-m>
            <x-m> -0.0004585 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
        <visible>true</visible>
        <drag-scale-px>20</drag-scale-px>
        <action>
            <binding>
                <command>property-adjust</command>
                <!--<property>instrumentation/transponder/inputs/digit[2]</property>-->
                <property alias="../../../../params/digit2"/>
                <factor>1</factor>
                <min>0</min>
                <max>8</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-squawk</tooltip-id>
                <label>Squawk: %.4d</label>
                <property>instrumentation/transponder/id-code</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Dial10</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[1]</property>-->
        <property alias="../../params/digit1"/>
        <factor>45</factor>
        <center>
            <z-m>  0.0028 </z-m>
            <y-m>  0.02485 </y-m>
            <x-m>  0.00079415 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>Knob10</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[1]</property>-->
        <property alias="../../params/digit1"/>
        <factor>45</factor>
        <center>
            <z-m>  0.010171 </z-m>
            <y-m>  0.02485 </y-m>
            <x-m> -0.0004585 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
        <visible>true</visible>
        <drag-scale-px>20</drag-scale-px>
        <action>
            <binding>
                <command>property-adjust</command>
                <!--<property>instrumentation/transponder/inputs/digit[1]</property>-->
                <property alias="../../../../params/digit1"/>
                <factor>1</factor>
                <min>0</min>
                <max>8</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-squawk</tooltip-id>
                <label>Squawk: %.4d</label>
                <property>instrumentation/transponder/id-code</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Dial1</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[0]</property>-->
        <property alias="../../params/digit0"/>
        <factor>45</factor>
        <center>
            <z-m>  0.0028 </z-m>
            <y-m>  0.04375 </y-m>
            <x-m>  0.00079415 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>Knob1</object-name>
        <!--<property>instrumentation/transponder/inputs/digit[0]</property>-->
        <property alias="../../params/digit0"/>
        <factor>45</factor>
        <center>
            <z-m>  0.010171 </z-m>
            <y-m>  0.04375 </y-m>
            <x-m> -0.0004585 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  0 </y>
            <z> -1 </z>
        </axis>
        <visible>true</visible>
        <drag-scale-px>20</drag-scale-px>
        <action>
            <binding>
                <command>property-adjust</command>
                <!--<property>instrumentation/transponder/inputs/digit[0]</property>-->
                <property alias="../../../../params/digit0"/>
                <factor>1</factor>
                <min>0</min>
                <max>8</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-squawk</tooltip-id>
                <label>Squawk: %.4d</label>
                <property>instrumentation/transponder/id-code</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>switchEnable</object-name>
        <visible>true</visible>
        <!--<drag-scale-px>20</drag-scale-px>-->
        <action>
            <binding>
                <command>property-cycle</command>
                <property>/instrumentation/transponder/inputs/knob-mode</property>
                <wrap>1</wrap>
                <value>0</value>
                <!-- OFF -->
                <value>5</value>
                <!-- ON  -->
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-enable-btn</tooltip-id>
                <label>Power</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>switchEnable</object-name>
        <property>/instrumentation/transponder/inputs/knob-mode</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>5</ind>
                <dep>-45</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>45</dep>
            </entry>
        </interpolation>
        <center>
            <z-m>  0.007028 </z-m>
            <y-m> -0.038577 </y-m>
            <x-m> -0.000602 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y>  1 </y>
            <z>  0 </z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>switchMode</object-name>
        <visible>true</visible>
        <!--<drag-scale-px>20</drag-scale-px>-->
        <action>
            <binding>
                <command>property-cycle</command>
                <property>/instrumentation/transponder/mode</property>
                <wrap>1</wrap>
                <value>0</value>
                <!-- A -->
                <value>1</value>
                <!-- C  -->
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-mode-btn</tooltip-id>
                <label>Mode</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>switchMode</object-name>
        <property>/instrumentation/transponder/mode</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-45</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>45</dep>
            </entry>
        </interpolation>
        <center>
            <z-m>  0.007028 </z-m>
            <y-m>  0.062832 </y-m>
            <x-m> -0.000602 </x-m>
        </center>
        <axis>
            <x>  0 </x>
            <y> -1 </y>
            <z>  0 </z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>test</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/transponder/inputs/test</property>
                <value>1</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>/instrumentation/transponder/inputs/test</property>
                    <value>0</value>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-test</tooltip-id>
                <label>Test transponder</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>test</object-name>
        <property>/instrumentation/transponder/inputs/test</property>
        <factor> 0.0013 </factor>
        <axis>
            <x> 0 </x>
            <y> 0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>ident</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/transponder/inputs/ident-btn</property>
                <value type="bool">true</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>/instrumentation/transponder/inputs/ident-btn</property>
                    <value type="bool">false</value>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>transponder-ident</tooltip-id>
                <label>Identify to ATC</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>ident</object-name>
        <property>/instrumentation/transponder/inputs/ident-btn</property>
        <factor> 0.0013 </factor>
        <axis>
            <x> 0 </x>
            <y> 0 </y>
            <z> -1 </z>
        </axis>
    </animation>

    <animation>
        <type>material</type>
        <object-name>lampRespond</object-name>
        <condition>
            <property>instrumentation/transponder/ident</property>
            <property>ja37/elec/ac-bus-main-bool</property>
        </condition>
        <emission>
            <red>   1.00 </red>
            <green> 1.00 </green>
            <blue>  1.00 </blue>
        </emission>
    </animation>

</PropertyList>
