<?xml version="1.0"?>

<PropertyList>

    <name type="string" n="0">cruise</name>

    <overlay>
        <sim>
          <presets>
            <speed-set type="string">knots</speed-set>
            <airspeed-kt type="int">300</airspeed-kt>
            <pitch-deg type="double">0.0</pitch-deg>
            <roll-deg type="double">0</roll-deg>
            <uBody-fps type="double">263</uBody-fps>
            <vBody-fps type="double">0</vBody-fps>
            <wBody-fps type="double">0</wBody-fps>
            <glideslope-deg type="double">0</glideslope-deg>
            <trim type="bool">true</trim>
            <onground type="bool">false</onground>
          </presets>
        </sim>
        <fdm>
            <jsbsim>
                <names>
                    <canopy>
                        <pos-norm type="double">0</pos-norm>
                    </canopy>
                    <gear-pos-norm type="double">0</gear-pos-norm>
                </names>
                <gear>
                    <gear-cmd-norm type="bool">true</gear-cmd-norm>
                </gear>
                <fcs>
                    <canopy>
                        <engage type="bool">false</engage>
                    </canopy>
                </fcs>
                <propulsion>
                    <engine>
                        <cutoff-commanded type="bool">false</cutoff-commanded>
                    </engine>
                </propulsion>
                <systems>
                    <external>
                        <switch type="bool">false</switch>
                        <enable-cmd type="bool">false</enable-cmd>
                    </external>
                    <electrical>
                        <generator-takeoff type="bool">true</generator-takeoff>
                    </electrical>
                </systems>
            </jsbsim>
        </fdm>
        <controls>
            <oxygen type="bool">true</oxygen>
            <altimeter-radar type="bool">true</altimeter-radar>
            <ventilation>
                <airconditioning-enabled type="bool">true</airconditioning-enabled>
            </ventilation>
            <engines>
                <engine n="0">
                    <starter-cmd type="bool">true</starter-cmd>
                    <cutoff type="bool">false</cutoff>
                    <reverser-cmd type="bool">false</reverser-cmd>
                    <throttle-cutoff type="bool">false</throttle-cutoff>
                </engine>
            </engines>
            <electric>
                <engine n="0">
                    <generator type="bool">true</generator>
                </engine>
                <main type="bool">true</main>
                <battery type="bool">false</battery>
                <reserve type="bool">false</reserve>
                <lights-ext-beacon type="bool">true</lights-ext-beacon>
                <lights-ext-nav type="int">1</lights-ext-nav>
                <lights-ext-form type="int">1</lights-ext-form>
                <lights-land-switch type="int">1</lights-land-switch>
                <main type="bool">true</main>
            </electric>
            <fuel>
                <auto type="bool">true</auto>
            </fuel>
            <gear>
                <gear-down type="bool">false</gear-down>
            </gear>
        </controls>
        <engines>
            <engine n="0">
                <running type="bool">true</running>
            </engine>
        </engines>
        <ja37>
            <mode>
                <selector-ajs type="int">2</selector-ajs><!-- NAV -->
            </mode>
            <radar>
                <enabled type="bool">true</enabled>
            </radar>
            <systems>
                <state type="string">cruise</state>
            </systems>
            <hud>
                <units-metric type="bool">false</units-metric>
            </hud>
            <avionics>
                <!--<master-warning-button type="bool">true</master-warning-button>-->
            </avionics>
        </ja37>
    </overlay>

</PropertyList>
